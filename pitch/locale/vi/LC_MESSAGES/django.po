#: admin.py:80 admin.py:99 views.py:115
msgid "Notice to order a pitch from Pitch App"
msgstr "Thông báo đặt sân bóng từ Pitch App"

#: forms.py:23
msgid "Start time must be greater than current time."
msgstr "Thời gian bắt đầu phải lớn hơn thời gian hiện tại."

#: forms.py:31
msgid "Invalid time start values."
msgstr "Lỗi thời gian bắt đầu."

#: forms.py:45
msgid "This time someone ordered! Please choose another time."
msgstr "Thời gian này đã được đặt. Vui lòng chọn thời gian khác!"

#: forms.py:49
msgid "Choose a larger time starting at least 1 hour."
msgstr "Lớn hơn thời gian bắt đầu tối thiếu 1 tiếng."

#: forms.py:61
msgid "Invalid time start and time end values."
msgstr "Thời gian bắt đầu hoặc kết thúc không chính xác."

#: forms.py:65
msgid "The minimum amount has not been reached to apply the voucher."
msgstr "Đơn hàng không đạt giá trị tối thiểu của voucher để có thể sử dụng."

#: forms.py:93
msgid "Once the order has been confirmed, it cannot be cancelled."
msgstr "Đơn hàng đã được xác nhận. Vì thế không thể hủy bỏ."

